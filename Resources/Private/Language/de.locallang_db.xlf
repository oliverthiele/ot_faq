			<trans-unit id="flexform.initialView.description" resname="flexform.initialView.description">
				<source>Default: First accordion items is opened, all others closed.</source>
				<target>Standard: Das erste Akkordeon-Element wird geöffnet, alle anderen werden geschlossen.</target>
			</trans-unit>
			<trans-unit id="flexform.initialView.0" resname="flexform.initialView.0">
